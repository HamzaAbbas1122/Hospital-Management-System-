  <property name="styleSheet">
   <string notr="true">/* Modern, professional styling */
QMainWindow {
    background-color: #f5f7fa;
    font-family: 'Segoe UI', Arial, sans-serif;
    color: #374151; /* Default text color */
}

QTabWidget::pane {
    border: 1px solid #d1d5db;
    border-radius: 6px;
    margin: 8px;
    background: white;
}

QTabBar::tab {
    padding: 8px 16px;
    background: #e5e7eb;
    border: 1px solid #d1d5db;
    border-bottom: none;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    color: #374151;
}

QTabBar::tab:selected {
    background: white;
    border-bottom: 2px solid #3b82f6;
    color: #1e40af;
    font-weight: bold;
}

QTableWidget {
    background: white;
    border: 1px solid #d1d5db;
    border-radius: 6px;
    alternate-background-color: #f9fafb;
    selection-background-color: #3b82f6;
    selection-color: white;
    gridline-color: #e5e7eb;
    color: #374151; /* Table text color */
}

QHeaderView::section {
    background-color: #f3f4f6;
    padding: 8px;
    border: none;
    font-weight: bold;
    color: #374151;
}

QPushButton {
    padding: 8px 16px;
    border-radius: 6px;
    border: 1px solid #d1d5db;
    background-color: white;
    min-width: 80px;
    color: #374151;
}

QPushButton:hover {
    background-color: #f3f4f6;
}

QPushButton:pressed {
    background-color: #e5e7eb;
}

QPushButton:disabled {
    background-color: #f3f4f6;
    color: #9ca3af;
}

QPushButton#primaryButton {
    background-color: #3b82f6;
    color: white;
    border: 1px solid #2563eb;
}

QPushButton#primaryButton:hover {
    background-color: #2563eb;
}

QPushButton#dangerButton {
    background-color: #ef4444;
    color: white;
    border: 1px solid #dc2626;
}

QPushButton#dangerButton:hover {
    background-color: #dc2626;
}

QLineEdit, QDateEdit, QComboBox, QSpinBox, QDoubleSpinBox {
    padding: 8px;
    border: 1px solid #d1d5db;
    border-radius: 6px;
    background: white;
    min-height: 30px;
    color: #374151;
}

QTextEdit {
    border: 1px solid #d1d5db;
    border-radius: 6px;
    background: white;
    padding: 4px;
    color: #374151;
}

QFrame {
    background: white;
    border-radius: 6px;
}

QLabel {
    color: #374151;
}

QGroupBox {
    border: 1px solid #d1d5db;
    border-radius: 6px;
    margin-top: 10px;
    padding-top: 15px;
    font-weight: bold;
    color: #374151;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px;
    color: #374151;
}

QStatusBar {
    background: #f3f4f6;
    border-top: 1px solid #d1d5db;
    color: #374151;
}

QMenuBar {
    background: white;
    border-bottom: 1px solid #d1d5db;
    color: #374151;
}

QMenuBar::item {
    padding: 4px 8px;
    background: transparent;
    color: #374151;
}

QMenuBar::item:selected {
    background: #e5e7eb;
    border-radius: 4px;
    color: #374151;
}

QMenu {
    background: white;
    border: 1px solid #d1d5db;
    padding: 4px;
    color: #374151;
}

QMenu::item:selected {
    background-color: #3b82f6;
    color: white;
}

/* Fix for all QLabel text color */
QLabel {
    color: #374151;
}

/* Fix for QTableWidget text color */
QTableWidget QTableCornerButton::section {
    background-color: #f3f4f6;
    color: #374151;
}

/* Fix for QHeaderView text color */
QHeaderView {
    color: #374151;
}

/* Fix for QGroupBox title text color */
QGroupBox::title {
    color: #374151;
}

/* Fix for QTabWidget text color */
QTabWidget {
    color: #374151;
}

/* Fix for QComboBox text color */
QComboBox {
    color: #374151;
}

QComboBox QAbstractItemView {
    color: #374151;
    background: white;
}

/* Fix for QDateEdit text color */
QDateEdit {
    color: #374151;
}

/* Fix for QSpinBox/QDoubleSpinBox text color */
QSpinBox, QDoubleSpinBox {
    color: #374151;
}
</string>
